{
    "extends": [
        /*"@rocketseat/eslint-config/react"*/
    ],
    
    "rules": {
        "camelcase": "off",
        "no-unused-vars" : 2,
        "no-undef" : 2
    },

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "ecmaFeatures": {
        "modules": true,
        "spread" : true,
        "restParams" : true
    },
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    }
    
}